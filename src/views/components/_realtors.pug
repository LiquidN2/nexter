-
  var path = require('path');
  var realtors = [
    {
      name: 'Erik Feinman',
      imgUrl: 'img/realtor-1.jpeg',
      imgAlt: 'Realtor 1',
      housesSold: 245
    }, {
      name: 'Kim Brown',
      imgUrl: 'img/realtor-2.jpeg',
      imgAlt: 'Realtor 2',
      housesSold: 212
    }, {
      name: 'Toby Ramsey',
      imgUrl: 'img/realtor-3.jpeg',
      imgAlt: 'Realtor 3',
      housesSold: 198
    }
  ];

.realtors
  h3.heading-3 Top 3 realtors
  .realtors__list
    each realtor in realtors
      img.realtors__img(src=`${realtor.imgUrl}`, alt=`${realtor.imgAlt}`)/
      .realtors__details
        h4.heading-4.heading-4--light #{ realtor.name }
        p.realtors__sold #{ realtor.housesSold } #{ realtor.housesSold > 1 ? 'houses' : 'house' } sold
